@{
    Layout = "_LayoutPrint";
    var record_id = 1;
}



<table id="data_header" width="96%" cellspacing="0" cellpadding="8" style="font-size: 12px">
    <tbody>
        <tr>
            <td style="vertical-align: top;text-align: left">
                <p>Tỉnh/thành phố: Quảng Bình</p>
                <p>Quận/huyện/thị xã: @ViewData["xa"]</p>
                <p>Xã/phường/thị trấn: @ViewData["huyen"]</p>
            </td>
            <td class="text-right" style="font-style:italic">Mẫu số 01a</td>
        </tr>

        <tr>
            <td colspan="2">
                <p style="text-align: center;font-weight: bold;font-size: 20px; text-transform: uppercase;">
                    PHIẾU THÔNG TIN
                    VỀ NGƯỜI
                    LAO ĐỘNG
                </p>
                <p style="text-align: center;font-style: italic;">
                    (Thu thập thông tin người từ 15 tuổi trở lên có nhu cầu
                    tìm
                    kiếm việc làm đang thực tế thường trú
                    tại địa bàn)
                </p>
            </td>
        </tr>
    </tbody>
</table>
<table id="data_body" cellspacing="0" cellpadding="0" style="font-size: 12px">
    <tbody>
        <tr>
            <td style="text-align: left"><b>1.Họ, chữ đệm và tên khai sinh:</b> @ViewData["hoten"] </td>
        </tr>
        <tr>
            <td style="text-align: left"><b>2.Ngày, tháng, năm sinh:</b> @ViewData["ngaysinh"] </td>
            <td style="text-align: left">
                <b>3.Giới tính:</b>
                Nam
            </td>
        </tr>
        <tr>
            <td style="text-align: left"><b>4. Số CCCD/CMND: </b> @ViewData["cccd"] </td>
        </tr>
        <tr>
            <td style="text-align: left"><b>5.Số điện thoại liên hệ</b> @ViewData["sdt"] </td>
        </tr>
        <tr>
            <td style="text-align: left"><b>6.Nơi đăng ký hộ khẩu thường trú:</b>  @ViewData["thuongtru"]</td>
        </tr>
        <tr>
            <td style="text-align: left"><b>7.Nơi ở hiện tại: </b> @ViewData["thuongtru"] </td>
        </tr>
        <tr>
            <td style="text-align: left">
                <b>7.1 Khu vực:</b>
                ☐ Thành thị  ☑ Nông thôn
            </td>
        </tr>
        <tr>
            <td style="text-align: left">
